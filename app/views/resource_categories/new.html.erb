<%= title [l(:label_resource_category_plural), settings_project_path(@project, tab: 'resource_categories')], l(:label_resource_category_new) %>

<%= labelled_form_for @resource_category, url: project_resource_categories_path(@project) do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <p>
    <%= submit_tag l :button_create %>
    <%= submit_tag l(:button_create_and_continue), name: 'continue' %>
  </p>
<% end %>


