<% if issue && User.current.allowed_to?(:view_issue_supply_items, issue.project) && issue.issue_supply_items.any? %>
  <%= issue_fields_rows do |rows|
    rows.left l(:label_supply_item_plural), safe_join(RedmineSupply::IssueSupplyItemsPresenter.new(issue.issue_supply_items).call, ', ')
  end %>
<% end %>

