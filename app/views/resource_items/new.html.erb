<%= title [l(:label_resource_item_plural), settings_project_path(@project, tab: 'resource_items')], l(:label_resource_item_new) %>

<%= labelled_form_for @resource_item, url: project_resource_items_path(@project) do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <p>
    <%= submit_tag l :button_create %>
    <%= submit_tag l(:button_create_and_continue), name: 'continue' %>
  </p>
<% end %>


