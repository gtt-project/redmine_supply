<%= title [l(:label_supply_item_plural), settings_project_path(@project, tab: 'supply_items')], l(:label_supply_item_new) %>

<%= labelled_form_for @supply_item, url: project_supply_items_path(@project) do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <p>
    <%= submit_tag l :button_create %>
    <%= submit_tag l(:button_create_and_continue), name: 'continue' %>
  </p>
<% end %>


